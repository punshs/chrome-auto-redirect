{
  "key": "",
  "manifest_version": 3,
  "name": "Auto Page Redirector",
  "version": "1.0",
  "description": "Automatically redirects to a specified page after a set time",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": { "matches": ["*://*/*"] }
}